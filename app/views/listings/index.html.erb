<h1>Listings</h1>

<%= link_to 'New Listing', new_listing_path%>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th colspan="3"></th>
  </tr>
 
  <% @listing.each do |article| %>
    <tr>
      <td><%= article.listing %></td>
	  <td><%= article.description %></td>

    </tr>
  <% end %>
</table>